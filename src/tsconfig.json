{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "importHelpers": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "removeComments": true,
    "noEmitHelpers": true,
    "noLib": false,
    "outDir": "dist/",
    "preserveConstEnums": true
  }
}